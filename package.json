{
  "name": "ember-cli-deploy-sql",
  "version": "1.1.0",
  "description": "Deploy your Ember.js index to a database table",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "coveralls": "./node_modules/.bin/coveralls < ./coverage/lcov.info",
    "test": "./node_modules/.bin/eslint index.js tests/**/*.js && ./node_modules/.bin/istanbul cover -x migrations/**/*.js tests/runner.js",
    "toc": "doctoc --notitle --maxlevel 2 --github README.md"
  },
  "repository": "https://github.com/mwpastore/ember-cli-deploy-sql",
  "engines": {
    "node": ">= 4"
  },
  "author": "Mike Pastore <mike@oobak.org>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "~3.5.0",
    "core-object": "^3.1.5",
    "ember-cli-deploy-plugin": "^0.2.9",
    "knex": "^0.14.6"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.0",
    "coveralls": "^3.0.0",
    "ember-cli": "3.1.4",
    "eslint": "^4.9",
    "glob": "^7.1.1",
    "istanbul": "~0.4.5",
    "mocha": "^5.2.0",
    "rewire": "^3.0.0",
    "sqlite3": "^4.0.0"
  },
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin",
    "mysql",
    "mariadb",
    "postgres",
    "sqlite3",
    "oracle",
    "mssql",
    "knex"
  ]
}
